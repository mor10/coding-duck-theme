{
  "name": "Coding Duck Light",
  "colors": {
    "editor.background": "#F5F2E8",
    "editor.foreground": "#4A4234",
    "editor.selectionBackground": "#E6D5C0",
    "editor.selectionHighlightBackground": "#ebe2d380",
    "editor.lineHighlightBackground": "#EBE2D3",
    "editorCursor.foreground": "#4A4234",
    "editorWhitespace.foreground": "#D4C7B4",
    "editor.findMatchBackground": "#E6D5C0",
    "editor.findMatchHighlightBackground": "#ebe2d380",
    "editorLineNumber.foreground": "#8B7B6A",
    "editorLineNumber.activeForeground": "#4A4234",
    "list.activeSelectionBackground": "#E6D5C0",
    "list.activeSelectionForeground": "#4A4234",
    "list.hoverBackground": "#EBE2D3",
    "list.hoverForeground": "#4A4234",
    "sideBar.background": "#F5F2E8",
    "sideBar.foreground": "#4A4234",
    "sideBarTitle.foreground": "#4A4234",
    "titleBar.activeBackground": "#F5F2E8",
    "statusBar.background": "#E6D5C0",
    "statusBar.foreground": "#4A4234"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#8B7B6A"
      }
    },
    {
      "name": "Comments: Preprocessor",
      "scope": "comment.block.preprocessor",
      "settings": {
        "fontStyle": "",
        "foreground": "#8B7B6A"
      }
    },
    {
      "name": "Comments: Documentation",
      "scope": ["comment.documentation", "comment.block.documentation"],
      "settings": {
        "foreground": "#6B5D3D"
      }
    },
    {
      "name": "Invalid - Illegal",
      "scope": "invalid.illegal",
      "settings": {
        "foreground": "#A13131"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#725E45"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "storage"],
      "settings": {
        "foreground": "#725E45"
      }
    },
    {
      "name": "Types",
      "scope": ["storage.type", "support.type"],
      "settings": {
        "foreground": "#5A4E8C"
      }
    },
    {
      "name": "Language Constants",
      "scope": ["constant.language", "support.constant", "variable.language"],
      "settings": {
        "foreground": "#855C30"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "support.variable"],
      "settings": {
        "foreground": "#6B5D3D"
      }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#96690A"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class",
        "support.class"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#5A4E8C"
      }
    },
    {
      "name": "Exceptions",
      "scope": "entity.name.exception",
      "settings": {
        "foreground": "#A13131"
      }
    },
    {
      "name": "Numbers, Characters",
      "scope": ["constant.numeric", "constant.character", "constant"],
      "settings": {
        "foreground": "#855C30"
      }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "foreground": "#2F657F"
      }
    },
    {
      "name": "Strings: Escape Sequences",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#725E45"
      }
    },
    {
      "name": "Strings: Regular Expressions",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#2F657F"
      }
    },
    {
      "name": "Strings: Symbols",
      "scope": "constant.other.symbol",
      "settings": {
        "foreground": "#855C30"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation",
      "settings": {
        "foreground": "#725E45"
      }
    },
    {
      "name": "HTML: Doctype Declaration",
      "scope": [
        "meta.tag.sgml.doctype",
        "meta.tag.sgml.doctype string",
        "meta.tag.sgml.doctype entity.name.tag",
        "meta.tag.sgml punctuation.definition.tag.html"
      ],
      "settings": {
        "foreground": "#8B7B6A"
      }
    },
    {
      "name": "HTML: Tags",
      "scope": [
        "meta.tag",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": {
        "foreground": "#486A9A"
      }
    },
    {
      "name": "HTML: Tag Names",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#486A9A"
      }
    },
    {
      "name": "HTML: Attribute Names",
      "scope": [
        "meta.tag entity.other.attribute-name",
        "entity.other.attribute-name.html"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#7D6C4E"
      }
    },
    {
      "name": "HTML: Entities",
      "scope": ["constant.character.entity", "punctuation.definition.entity"],
      "settings": {
        "foreground": "#855C30"
      }
    },
    {
      "name": "CSS: Selectors",
      "scope": [
        "meta.selector",
        "meta.selector entity",
        "meta.selector entity punctuation",
        "entity.name.tag.css"
      ],
      "settings": {
        "foreground": "#5A4E8C"
      }
    },
    {
      "name": "CSS: Property Names",
      "scope": ["meta.property-name", "support.type.property-name"],
      "settings": {
        "foreground": "#855C30"
      }
    },
    {
      "name": "CSS: Property Values",
      "scope": [
        "meta.property-value",
        "meta.property-value constant.other",
        "support.constant.property-value"
      ],
      "settings": {
        "foreground": "#2F657F"
      }
    },
    {
      "name": "CSS: Important Keyword",
      "scope": "keyword.other.important",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup: Changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#4A4234"
      }
    },
    {
      "name": "Markup: Deletion",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#A13131"
      }
    },
    {
      "name": "Markup: Insertion",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#2F657F"
      }
    },
    {
      "name": "Markup: Link",
      "scope": "meta.link",
      "settings": {
        "foreground": "#486A9A"
      }
    },
    {
      "name": "Markup: Output",
      "scope": ["markup.output", "markup.raw"],
      "settings": {
        "foreground": "#8B7B6A"
      }
    },
    {
      "name": "Markup: Heading",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#96690A"
      }
    },
    {
      "name": "Markup: Quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#5A4E8C"
      }
    },
    {
      "name": "Markup: Lists",
      "scope": "markup.list",
      "settings": {
        "foreground": "#486A9A"
      }
    },
    {
      "name": "Markup: Styling",
      "scope": ["markup.bold", "markup.italic"],
      "settings": {
        "foreground": "#2F657F"
      }
    },
    {
      "name": "Markup: Inline",
      "scope": "markup.inline.raw",
      "settings": {
        "fontStyle": "",
        "foreground": "#855C30"
      }
    }
  ]
}
